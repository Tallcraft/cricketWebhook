    "eslint-plugin-import": "^2.13.0",
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
